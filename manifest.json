{
	"manifest_version": 2,
	"name": "Wph",
	"version": "1.0",
	"description": "Ayuda para los blog de la Universidad",
	"permissions": [
		"contextMenus",
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"64": "img/umh64.png",
		"32": "img/umh32.png",
		"16": "img/umh16.png",
		"128": "img/umh128.png"
	},
	"content_scripts": [{
		"matches": [
			"http://*.umh.es/*",
			"https://*.umh.es/*"
		],
		"js": ["js/jquery.min.js", "js/script_page.js"]
	}]
}
